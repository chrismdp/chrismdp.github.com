---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>James Hunt</title>
 <link href="http://tooky.github.com/eden_atom.xml" rel="self"/>
 <link href="http://tooky.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ohthatjames.github.com/</id>
 <author>
   <name>James Hunt</name>
   <email>ohthatjames@gmail.com</email>
 </author>
 
 {% for post in site.categories.eden %}
 <entry>
   <title>{{ post.title }}</title>
   <tags>{{post.tags}}</tags>
   {% for category in post.categories %}
   <category term="{{category}}" />
   {% endfor %}
   <link href="http://ohthatjames.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ohthatjames.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>